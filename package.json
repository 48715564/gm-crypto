    "to-arraybuffer": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "ava": "^3.13.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2"
  },
  "config": {
    "commitizen": {
